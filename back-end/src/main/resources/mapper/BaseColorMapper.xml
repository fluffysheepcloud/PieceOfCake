<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csulb.cookie.mapper.BaseColorMapper">

    <resultMap id="BaseResultMap" type="com.csulb.cookie.domain.BaseColor">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="mId" column="m_id" jdbcType="INTEGER"/>
            <result property="color" column="color" jdbcType="VARCHAR"/>
            <result property="colorPrice" column="color_price" jdbcType="DOUBLE"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,m_id,color,
        color_price
    </sql>
</mapper>
